<div class="row text-center">
  <button id="recalls-btn" type="submit" class="btn btn-primary">Get recalls</button>
</div>

{{#if jsonResults.length}}
<div class="col-12 recall list">
  <h2>Your Recalls:</h2>
 
 {{#each jsonResults as |result| }}
      <p>{{result.status}}</p>
    {{/each}}
</div>
{{/if}}

<script src="/js/addRecall.js"></script>
