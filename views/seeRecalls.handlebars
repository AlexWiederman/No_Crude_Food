<div class="row text-center">
  <button id="recalls-btn" type="submit" class="btn btn-primary">Get recalls</button>
</div>

{{#if datas.length}}
<div class="col-12 recall list">
  <h2>Your Recalls:</h2>
    {{#each datas as |data|}}
      {{> commentRecall.handlebars}}
    {{/each}}

</div>
{{/if}}

<script src="/javascript/add-recall.js"></script>
